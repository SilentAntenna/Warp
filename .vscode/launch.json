    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序",
            "program": "${workspaceRoot}/main.js",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "C:/WorkProject/Recharge/node_modules/electron/dist/electron.exe",
            "stopOnEntry": false,
            "preLaunchTask": null,
            "env": {
                "NODE_ENV": "development"
            },
            "args": [
                "-i",
                "test"
            ],
            "console": "integratedTerminal",
            "sourceMaps": false
        },
        {
            "type": "node",
            "request": "launch",
            "name": "调试程序",
            "program": "${workspaceRoot}/main.js",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "C:/WorkProject/Recharge/node_modules/electron/dist/electron.exe",
            "stopOnEntry": true,
            "args": [
                "-i",
                "test"
            ],
            "preLaunchTask": null,
            "env": {
                "NODE_ENV": "development"
            },
            "console": "integratedTerminal",
            "sourceMaps": false
        },
        {
            "type": "node",
            "request": "attach",
            "name": "附加到进程",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null
        }
    ]
